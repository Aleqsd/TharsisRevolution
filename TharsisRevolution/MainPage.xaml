    mc:Ignorable="d">
    <RelativePanel HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Width="1920" Height="1080" >
        <RelativePanel.Background>
            <ImageBrush Stretch="Fill" ImageSource="Assets/Space.png"/>
        </RelativePanel.Background>
        <Image gif:AnimationBehavior.SourceUri="/Assets/original.gif" gif:AnimationBehavior.RepeatBehavior="Forever" Margin="-228,-852,-2226,-198" />
        <Border x:Name="block_PvShip" BorderBrush="Gray" BorderThickness="5" RenderTransformOrigin="0.5,0.5"  UseLayoutRounding="False" d:LayoutRounding="Auto" Margin="-123,-108,123,108">
            <Border.RenderTransform>
                <CompositeTransform SkewX="50"/>
            </Border.RenderTransform>
            <Rectangle Fill="Black" Width="500" Height="200" RenderTransformOrigin="1,1"/>
        </Border>
        <ProgressBar x:Name="pg_PvShip" HorizontalAlignment="Left" Height="40" Margin="57,48,-57,-38" VerticalAlignment="Top" Width="282" Maximum="10" SmallChange="1" Value="0" RenderTransformOrigin="0.733,1.347" Foreground="White" BorderBrush="Black" Background="#33FFFFFF" ValueChanged="progressBar_PvShip"/>

        <Border x:Name="block_Semaines" BorderBrush="Gray" BorderThickness="5" Margin="705,-122,-705,122" RenderTransformOrigin="0.5,0.5">
            <Rectangle Fill="Black" Width="500" Height="200"/>
        </Border>
        <TextBlock x:Name="lbl_TimeSemaine" Foreground="White" Height="40" Text="1"  Width="40"  RelativePanel.AlignHorizontalCenterWithPanel="True" FontSize="30" FontFamily="Showcard Gothic"  RelativePanel.LeftOf="lbl_txtSemaine" Margin="3,4,-3,-4"/>
        <TextBlock x:Name="lbl_TimeSemaineMax" Foreground="White" Height="40" Text="/ 10"  RelativePanel.AlignHorizontalCenterWithPanel="True" FontSize="30" FontFamily="Showcard Gothic"  RelativePanel.LeftOf="lbl_txtSemaine" Margin="37,4,-57,-4"/>
        <Slider x:Name="slider_Semaines" IsEnabled="False" Height="50" VerticalAlignment="Top" Width="302" RelativePanel.AlignHorizontalCenterWithPanel="True" LargeChange="1" BorderThickness="0" Margin="18,42,-18,-42" Foreground="White" Background="White" Maximum="10" ValueChanged="slider_TimeSemaine_ValueChanged" Value="10"/>
        <TextBlock x:Name="lbl_txtSemaine" Foreground="White" Height="40" Text="semaines avant Mars"  Width="350"  RelativePanel.AlignHorizontalCenterWithPanel="True" FontSize="30" FontFamily="Showcard Gothic" Margin="80,4,-80,-4"/>

