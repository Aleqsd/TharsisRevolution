                <TextBlock x:Name="lbl_PvDocteur" Width="20px" Text="0" RelativePanel.AlignHorizontalCenterWith="pb_PvDocteur" RelativePanel.AlignVerticalCenterWith="pb_PvDocteur" Margin="1,-2,-1,2" ></TextBlock>
                <Grid x:Name="gr_DesDocteur" Width="90" Height="14" RelativePanel.Below="pb_PvDocteur" RelativePanel.AlignHorizontalCenterWithPanel="True" Background="Black" Padding="0" Margin="0">
